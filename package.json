{
	"name": "deprecations",
	"version": "1.0.5",
	"author": "Jordan Harband",
	"description": "Given an npm module name, get a map of npm version numbers to deprecation messages.",
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"test": "npm run lint && node test/index.js",
		"coverage": "covert test/*.js",
		"coverage-quiet": "covert test/*.js --quiet",
		"lint": "jscs *.js test/*.js"
	},
	"preferGlobal": true,
	"bin": {
		"deprecations": "bin.js"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/ljharb/npm-deprecations.git"
	},
	"keywords": [
		"deprecations",
		"npm",
		"deprecated",
		"deprecation messages"
	],
	"dependencies": {
		"npm": "^2.7.4",
		"object.assign": "^1.1.1",
		"promiseback": "^2.0.1"
	},
	"devDependencies": {
		"tape": "^3.5.0",
		"covert": "^1.0.1",
		"jscs": "^1.11.3"
	},
	"engines": {
		"node": ">= 0.4"
	}
}

